<h1>Orders</h1>
<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>User</th>
      <th>Total Price</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% orders.forEach(order => { %>
      <tr>
        <td><%= order._id %></td>
        <td><%= order.user.username %></td>
        <td><%= order.totalPrice %></td>
        <td><%= order.status %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
